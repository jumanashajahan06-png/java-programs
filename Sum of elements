 public class Main{
    public static void main(String[] args){
        int[] a ={1,2,3,4,5};
        
        int sum = 0;
        
        for(int i=0; i<a.length; i++){
           sum += a[i];
         }
        
        // for-each loop 
        
        for(int number : a){
            sum += number;
        }
        
        System.out.println("Sum of all the elements :" + sum);
    }
}
